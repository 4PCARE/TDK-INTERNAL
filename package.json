  "scripts": {
    "dev": "concurrently \"npm run legacy\" \"npm run gateway\" \"npm run auth-svc\" \"npm run doc-ingest\"",
    "legacy": "cd server && npx tsx index.ts",
    "gateway": "cd services/api-gateway && npm run dev",
    "auth-svc": "cd services/auth-svc && npm run dev",
    "doc-ingest": "cd services/doc-ingest-svc && npm run dev",
    "search-svc": "cd services/search-svc && npm run dev",
    "agent-svc": "cd services/agent-svc && npm run dev",
    "embedding-svc": "cd services/embedding-svc && npm run dev",
    "csat-svc": "cd services/csat-svc && npm run dev",
    "health-monitor": "cd services/health-monitor-svc && npm run dev",
    "services": "concurrently \"npm run gateway\" \"npm run auth-svc\" \"npm run doc-ingest\" \"npm run agent-svc\"",
    "build": "npm run build:legacy && npm run build:gateway && npm run build:services",
    "build:legacy": "cd server && npm run build",
    "build:gateway": "cd services/api-gateway && npm run build",
    "build:services": "cd services/auth-svc && npm run build && cd ../doc-ingest-svc && npm run build && cd ../agent-svc && npm run build",
    "start": "npm run gateway",
    "discover": "tsx scripts/service-discovery.ts",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting to be configured'",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate"
  },
